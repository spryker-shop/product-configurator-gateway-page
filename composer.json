{
  "name": "spryker-shop/product-configurator-gateway-page",
  "description": "ProductConfiguratorGatewayPage module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/shop-application": "*",
    "spryker/application": "*",
    "spryker/kernel": "^3.30.0",
    "spryker/product-configuration": "^0.1.0",
    "spryker/product-configuration-storage": "^0.1.0",
    "spryker/quote": "^2.8.0",
    "spryker/symfony": "^3.1.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/router": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "spryker/router": "ADD SUGGEST DESCRIPTION"
  }
}
